<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.a.ycphack2018">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/ycpapptheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".searchprojectsActivity" />
        <activity android:name=".AccountTypeActivity" />
        <activity android:name=".CreatorAccountActivity" />
        <activity android:name=".EditProfileActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".NewProjectActivity" />
        <activity android:name=".MyProjectsActivity" />
        <activity android:name=".HomeMenuActivity" />
        <activity android:name=".ViewProfileActivity" />
        <activity android:name=".ViewProjectActivity"></activity>
    </application>

</manifest>